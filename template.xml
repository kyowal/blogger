<?xml version="1.0" encoding="UTF-8" ?>
    <!DOCTYPE html>
    <html lang='en' xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>

    <head>
        <meta content='width=device-width, initial-scale=1.0' name='viewport' />
        <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"/>
        <link rel="stylesheet" type="text/css" href="https://use.fontawesome.com/releases/v5.0.8/css/all.css"/>
        <link rel="stylesheet" type="text/css" href="https://raw.githubusercontent.com/kyowal/blogger/development/style.css"/>
        <b:skin>
            <![CDATA[]]>
        </b:skin>
    </head>

    <body>
        <div class="header" id="header">
            <div class="logo">logo</div>
        </div>

        
                    <b:section class='main' id='main' showaddelement='no'>
                        <b:widget id='Blog1' locked='true' title='Blog Posts' type='Blog' version='1'>
                            <b:includable id='main' var='top'>
                                <b:if cond='data:blog.pageType == &quot;item&quot;'>
									<b:include name='header-post' />
                                    <!-- Layout for Single Page -->
                                    <b:loop values='data:posts' var='post' index='ind'>
                                        <b:include data='post' name='single-post' />
                                    </b:loop>
                                <b:else/>
                                    <b:if cond='data:blog.pageType == &quot;static_page&quot;'>
											<!-- Include Header -->
											<b:include name='header-page' />
											<!-- Layout for Static Page -->
											<b:loop values='data:posts' var='post' index='ind'>
												<b:include data='post' name='static-page' />
											</b:loop>
                                    <b:else/>
											<b:include name='header-home' />									
											<!-- Layout for Label Page -->
											<b:loop values='data:posts' reverse='true' var='post' index='ind'>
												<b:include data='post' name='labelpage' />
											</b:loop>
											<b:include name='card-ajax-load' />
                                    </b:if>
                                </b:if>
                            </b:includable>
							
							<!-- Header Home-->
							<b:includable id='header-home'>
                            
							</b:includable>
							
							<!-- Header Page-->
							<b:includable id='header-page'>
                            
							</b:includable>
							
							<!-- Header Post-->
							<b:includable id='header-post'>
                            
							</b:includable>
							
							
							
                            <!-- Block A: -->
                            <b:includable id='single-post' var='post'>
                                <div id="singleview">
                                    <div class="_smaincontent">
                                        <!-- Title -->
                                        <b:include name='title' />
                                        <!-- Advertisement -->
                                        <b:include name='advertise' />
                                        <!-- Navigation -->
                                        <b:include name='navigate' />
                                        <!-- Content -->
										<b:include name='content' />
                                        <!-- Navigation -->
                                        <b:include name='navigate' />
                                        <!-- Whatsapp Join -->
                                        <b:include name='wpjoin' />
                                        <!-- Facebook Like -->
                                        <b:include name='facebook' />										
                                        <!-- Related List -->
                                        <b:include name='single-related' />
									</div>
                                </div>
                            </b:includable>

								<!-- Block A -> Title : -->
								<b:includable id='title' var='post'>
									<div><data:post.title/></div>
								</b:includable>
								
								<!-- Block A -> Navigate : -->
								<b:includable id='navigate' var='post'>
									<div><data:post.title/></div>
								</b:includable>
								
								<!-- Block A -> Content : -->
								<b:includable id='content' var='post'>
									<div><data:post.body/></div>
								</b:includable>
								
								<!-- Block A -> WP Join : -->
								<b:includable id='wpjoin' var='post'>
									<div>WP</div>
								</b:includable>
								
								<!-- Block A -> Facebook : -->
								<b:includable id='facebook' var='post'>
									<div>Fb</div>
								</b:includable>
								
								<!-- Block A -> Facebook : -->
								<b:includable id='single-related' var='post'>
									<div class="_rntent">
                                        <div class="_ndchj">Read More...</div>
                                        <ul id="relatedcall"></ul>
                                    </div>
								</b:includable>
														
                            <!-- Block B: -->
                            <b:includable id='static-page' var='post'>
                                <div>
                                    <b:if cond='data:post.firstImageUrl'>
                                        <img expr:src='data:post.firstImageUrl' />
                                    </b:if>
                                    <data:post.title/>
                                    <data:post.body/>
                                </div>
                            </b:includable>

                            <!-- Block C: -->
                            <b:includable id='labelpage' var='post'>
                                <div expr:class='&quot;_listouter list&quot; + data:ind'>
                                    <div class="_listpost">
                                        <b:include name='card-image' />
										<b:include name='card-title' />
										<b:include name='card-content' />
										<b:include name='card-strip' />
									</div>
                                </div>
                            </b:includable>
								<!-- Block C: Image -->
								<b:includable id='card-image' var='post'>
									<div class="">
										<b:if cond='data:post.firstImageUrl'>
											<img expr:src='data:post.firstImageUrl' />
										<b:else/>
											<img src='https://goo.gl/hWnykN' />
										</b:if>
                                    </div>
								</b:includable>
								
								<!-- Block C: Title -->
								<b:includable id='card-title' var='post'>
									<div>
										<data:post.title/>
									</div>
								</b:includable>
								<!-- Block C: Content -->
								<b:includable id='card-content' var='post'>
									<div class="">
										<data:post.body/>
									</div>
                                </b:includable>
								<!-- Block C: Strip -->
								<b:includable id='card-strip' var='post'>
									<div class="">
										<div expr:class='&quot;hidden indexer&quot; + data:ind' expr:data-link='data:post.url'>Read Now</div>
										<div expr:class='&quot;hidden indexer&quot; + data:ind' expr:data-title='data:post.title' expr:data-link='data:post.url'>Send to Friends</div>
                                    </div>
                                </b:includable>
								
							
                            <!-- Block D: -->
                            <b:includable id='card-ajax-load' var='post'>
                                <div class="hidden" id="_nextlist"><b:include name='nextprev' /></div>
                            </b:includable>

                            <!-- Block E: -->
                            <b:includable id='header' var='post'>

                            </b:includable>

                            <!-- Block F: -->
                            <b:includable id='footer' var='post'>

                            </b:includable>
							<!-- Block G: -->
                            <b:includable id='advertise' var='post'>
								
                            </b:includable>

                        </b:widget>
                    </b:section>
		<b:section id='footer' preferred='no'></b:section>
        <!-- This is a container Of next and Previous list -->
        <div id="previousload" class="hidden" data-oldurl=""></div>
        <div id="nextload" class="hidden" data-newurl=""></div>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.bundle.min.js"></script>
        <script async="true" src="https://www.google-analytics.com/analytics.js"></script>
        <script src="https://raw.githubusercontent.com/kyowal/blogger/development/script.js"></script>
    </body>
    </html>